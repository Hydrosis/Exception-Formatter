{
    "name": "stack-trace-formatter",
    "displayName": "Stack Trace Formatter",
    "description": "Formats C# and Java stack traces.",
    "version": "1.0.4",
    "publisher": "RadwanFaci",
    "repository": {
        "url": "https://github.com/Hydrosis/Exception-Formatter"
    },
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.formatStackTrace"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.formatStackTrace",
            "title": "Format Stack Trace"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}